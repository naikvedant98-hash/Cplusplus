#include <iostream>
using namespace std;

int doubleGame(int x, int num) {
    return num * x;
}

int main() {
    int num;
    cout << "Enter the value for table: ";
    cin >> num;

    for (int i = 1; i <= 10; i++) {
        cout << num << " * " << i << " = " << doubleGame(i, num) << endl;
    }

    return 0;
}

//Output
Enter the value for table: 5
5 * 1 = 5
5 * 2 = 10
5 * 3 = 15
5 * 4 = 20
5 * 5 = 25
5 * 6 = 30
5 * 7 = 35
5 * 8 = 40
5 * 9 = 45
5 * 10 = 50


=== Code Execution Successful ===
