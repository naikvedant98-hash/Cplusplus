#include <iostream>
using namespace std;

int main() {
    
    int num;
    
    cout << "Enter the value of num : ";
    cin >> num;
    
    if(num % 2 == 0){
        cout<<num<<" is an Even number";
    }else{
        cout<<num<<" is an Odd number";
    }
    return 0;
}

//Output
Enter the value of num : 6
6 is an Even number
